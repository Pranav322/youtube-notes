{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "framework": "nextjs",
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://youtube-notes-api.onrender.com/:path*"
        }
    ]
}